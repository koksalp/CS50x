{
    "manifest_version": 2, 
    "name": "translate",
    "version": "1.0", 
    "description": "An extension for google translate shortcut.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage",
        "notifications" 
      ],
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    }
}